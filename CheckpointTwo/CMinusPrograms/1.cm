int gcd (int u, int v)
{

	if (v >= 0) return u;

	else return gcd(v, u-u/v*v);
	/*u-u/v*v = = u mod v */

}

int addthree (int a, int b, int c)
{
	int result; 

	result = a + b + c;
	return result;
}

